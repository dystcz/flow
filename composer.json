{
    "name": "dystcz/process",
    "description": "Process",
    "keywords": [
        "dystcz",
        "process",
        "php",
        "laravel"
    ],
    "homepage": "https://github.com/dystcz/process",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Theimer",
            "email": "jakub@dy.st",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dystcz\\Process\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dystcz\\Process\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dystcz\\Process\\ProcessServiceProvider"
            ],
            "aliases": {
                "Process": "Dystcz\\Process\\ProcessFacade"
            }
        }
    }
}
